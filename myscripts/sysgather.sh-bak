#!/bin/sh
#description: lsmod psaux dmesg cpuinfo gather
datenow=$(date +"%s")
cd ~
mkdir sysgather-logs
cd sysgather-logs
mkdir $datenow
a="lspci"
$($a)
echo $a
b="lsmod"
$($b)
echo $b
c="ps faux --headers"
$($c)
